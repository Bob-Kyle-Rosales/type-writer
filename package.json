{
  "name": "type-writer",
  "version": "1.0.0",
  "private": true,
  "description": "Typewriter-style web document editor",
  "main": "server/index.js",
  "scripts": {
    "build": "cd client && npm install && npm run build",
    "start": "node server/index.js",
    "dev": "concurrently \"nodemon server/index.js\" \"cd client && npm start\""
  },
  "keywords": [
    "typewriter",
    "editor",
    "react",
    "node"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
